{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Nest.js",
        "program": "${workspaceFolder}/src/main.ts",
        "args": [],
        "cwd": "${workspaceFolder}",
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen"
      },
      {
        "type": "node",
        "request": "attach",
        "name": "Attach to Nest.js",
        "port": 3000
      }
    ]
  }